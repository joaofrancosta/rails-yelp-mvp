<h1>Restaurants</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Description</th>
      <th>Stars</th>
    </tr>
  </thead>
  <tbody>
    <% @restaurants.each do |restaurant|%>
    <tr>
      <td><%= link_to restaurant.name, restaurant %></td>
      <td><%= restaurant.address %></td>
    </tr>
    <% end %>
      <td><%= link_to 'New restaurant', new_restaurant_path %> | </td>
      <td><%= link_to 'Delete restaurant', restaurant_path(restaurant) %></td>

  </tbody>
</table>
